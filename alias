alias=cat C:\work\alias $*
grep=greps --color $*
ll=ls -l -h $* 
ps=tasklist $*
kill=taskkill /F /IM $*
open=explorer $*
o=explorer .
merge=svnmerge merge -s -v -S trunk/cedi -r $*
st=-Dmaven.test.skip=true
bumpup=mvn versions:set -DgenerateBackupPoms=false -DnewVersion=$*
host=cat C:\Windows\System32\drivers\etc\hosts $*
mvnst=mvn %st% $*
bdd=mvn test-compile test -Dtest=$*
mc=mvn clean
mci=mvn clean install
mcist=mvn clean install %st%
mi=mvn install
commit=svn commit --file svnmerge-commit-message.txt
show=svn log -l $* 
update=svn up .
revert=svn st | awk "{print $2;}" | xargs rm -rf && svn revert . 
man=explorer http://ss64.com/bash/$*.html
nobody=find . -user nobody -name '*.*' -exec rm -fv {} +
cleanup=find . -name '*.*' -exec rm -fv {} +
xargs=find . -name "*.rpt" | xargs grep "ST\*277" -l
esdoc-json=echo {"source": "./src", "destination": "./doc"} > .esdoc.json
lt=lt -I "node_modules" $*
n=npm $*
clear=cls $*
whereis=whereis $* && cat C:\work\alias | grep $*